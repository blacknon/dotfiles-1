[user]
  name = kozyty
  email = kozyty@gmail.com
  #email = taiyo.kojima@nanapi.co.jp

[color]
  ui = true
[http]
  sslVerify = false
[alias]
  st = status
  ss = "status -s"
  br = "branch -a"
  co = "checkout"
  unstage = "reset HEAD --"
  addu = "add --update"
  adda = "add --all"
  commit = "commit -av"

[core]
  editor = /usr/bin/vim -c \"set fenc=utf-8\"
  #editor = /usr/local/vim/bin/vim -c \"set fenc=utf-8\"
  excludesfile = ~/.gitignore
  whitespace = cr-at-eol
